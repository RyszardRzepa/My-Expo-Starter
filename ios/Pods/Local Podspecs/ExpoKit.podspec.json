{
  "name": "ExpoKit",
  "version": "1.16.2",
  "summary": "ExpoKit",
  "description": "ExpoKit allows native projects to integrate with the Expo SDK.",
  "homepage": "http://docs.expo.io",
  "license": "BSD",
  "authors": "650 Industries, Inc.",
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Core",
  "source": {
    "git": "http://github.com/expo/expo.git"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "ios/Exponent/**/*.{h,m}",
      "preserve_paths": "ios/Exponent/**/*.{h,m}",
      "exclude_files": [
        "ios/Exponent/EXAppDelegate.*",
        "ios/Exponent/EXRootViewController.*",
        "ios/Exponent/Supporting/**"
      ],
      "dependencies": {
        "Amplitude-iOS": [
          "~> 3.8"
        ],
        "Analytics": [
          "~> 3.5"
        ],
        "AppAuth": [
          "~> 0.4"
        ],
        "CocoaLumberjack": [
          "~> 3.0"
        ],
        "Crashlytics": [
          "~> 3.8"
        ],
        "FBAudienceNetwork": [
          "~> 4.22"
        ],
        "FBSDKCoreKit": [
          "~> 4.15"
        ],
        "FBSDKLoginKit": [
          "~> 4.15"
        ],
        "FBSDKShareKit": [
          "~> 4.15"
        ],
        "Fabric": [
          "~> 1.6"
        ],
        "Google/SignIn": [
          "~> 3.0"
        ],
        "GoogleMaps": [
          "~> 2.2.0"
        ],
        "lottie-ios": [
          "~> 1.5.1"
        ],
        "GPUImage": [
          "~> 0.1.7"
        ],
        "Branch": [
          "~> 0.14.12"
        ]
      }
    },
    {
      "name": "CPP",
      "dependencies": {
        "ExpoKit/Core": [

        ]
      },
      "source_files": [
        "cpp/*.{h,c,cpp,m,mm}",
        "cpp/**/*.{h,c,cpp,m,mm}"
      ]
    }
  ]
}
